<h1>Create a new route</h1>
<h2>Search for destinations and add them to the route</h2>



<%#= f.input :address, input_html: {data: {}, class: "d-none"} %>

<%= simple_form_for :search, url: root_path, method: "GET", html: { class: 'form-inline' } do |f| %>
  <%= f.input :address,
  input_html: {data: {address_autocomplete_target: "address"}},
  wrapper_html: {data: {controller: "address-autocomplete", address_autocomplete_api_key_value: ENV["MAPBOX_API_KEY"]}}
%>
  <%= f.submit "Search", class: "btn btn-primary" %>
<% end %>
